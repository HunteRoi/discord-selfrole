{
  "name": "@hunteroi/discord-selfrole",
  "version": "1.0.0",
  "description": "A framework to integrate a channel with a automated role-giver system inside your Discord bot built with DiscordJS",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\" --tab-width 4",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "example": "npm run build & node ./example/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hunteroi/discord-selfrole.git"
  },
  "keywords": [
    "selfrole",
    "discordjs",
    "roleme"
  ],
  "author": "HunteRoi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hunteroi/discord-selfrole/issues"
  },
  "homepage": "https://github.com/hunteroi/discord-selfrole#readme",
  "dependencies": {
    "discord.js": "^12.5.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.16",
    "@types/ws": "^7.4.0",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**/*"
  ]
}
